{
  "name": "expressjs-framework",
  "version": "0.2.0",
  "description": "A expressjs framework",
  "main": "dist",
  "scripts": {
    "build": "tsc"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "bammoo/expressjs-framework",
  "author": "John Xiao <xiao-jianfeng@qq.com>",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "fs-extra": "4.0.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "pino": "^5.11.3",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.13",
    "shortid": "^2.2.11"
  },
  "devDependencies": {
    "@types/async": "^2.0.45",
    "@types/node": "^9.4.6",
    "eslint": "^3.1.1",
    "pino-pretty": "^2.5.0",
    "typescript": "^3.3.3"
  }
}
